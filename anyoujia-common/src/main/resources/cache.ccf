jcs.region.areaCache.cacheattributes.MemoryCacheName=org.apache.commons.jcs.engine.memory.lru.LRUMemoryCache
jcs.region.areaCache.cacheattributes.UseMemoryShrinker=true
jcs.region.areaCache.cacheattributes.MaxMemoryIdleTimeSeconds=3600
jcs.region.areaCache.cacheattributes.ShrinkerIntervalSeconds=60
jcs.region.areaCache.cacheattributes.MaxSpoolPerRun=500
jcs.region.areaCache.elementattributes=org.apache.commons.jcs.engine.ElementAttributes
jcs.region.areaCache.elementattributes.IsEternal=false

jcs.region.idCache=DC
jcs.region.idCache.cacheattributes=org.apache.commons.jcs.engine.CompositeCacheAttributes
jcs.region.idCache.cacheattributes.MaxObjects=1000
jcs.region.idCache.cacheattributes.MemoryCacheName=org.apache.commons.jcs.engine.memory.lru.LRUMemoryCache
jcs.region.idCache.cacheattributes.UseMemoryShrinker=true
