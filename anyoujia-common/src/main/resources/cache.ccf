# DEFAULT CACHE REGION

jcs.default=DC
jcs.default.cacheattributes=org.apache.commons.jcs.engine.CompositeCacheAttributes
jcs.default.cacheattributes.MaxObjects=1000
jcs.default.cacheattributes.MemoryCacheName=org.apache.commons.jcs.engine.memory.lru.LRUMemoryCache

# PRE-DEFINED CACHE REGIONS

jcs.region.housingCache=DC
jcs.region.housingCache.cacheattributes=org.apache.commons.jcs.engine.CompositeCacheAttributes
jcs.region.housingCache.cacheattributes.MaxObjects=1000
jcs.region.housingCache.cacheattributes.MemoryCacheName=org.apache.commons.jcs.engine.memory.lru.LRUMemoryCache
jcs.region.housingCache.cacheattributes.UseMemoryShrinker=true
jcs.region.housingCache.cacheattributes.MaxMemoryIdleTimeSeconds=3600
jcs.region.housingCache.cacheattributes.ShrinkerIntervalSeconds=60
jcs.region.housingCache.cacheattributes.MaxSpoolPerRun=500
jcs.region.housingCache.elementattributes=org.apache.commons.jcs.engine.ElementAttributes
jcs.region.housingCache.elementattributes.IsEternal=false

